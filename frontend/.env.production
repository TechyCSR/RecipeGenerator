# Production Frontend Environment Variables
# Set these in Vercel dashboard under Settings > Environment Variables

# API Configuration
REACT_APP_API_URL=https://your-backend-deployment.vercel.app
REACT_APP_ENVIRONMENT=production

# Clerk Configuration (Live Keys)
REACT_APP_CLERK_PUBLISHABLE_KEY=pk_live_your_live_publishable_key

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# API Configuration
REACT_APP_SPOONACULAR_PROXY=true
